{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Seemodo Docs",
  "colors": {
    "primary": "#3B82F6",
    "light": "#60A5FA",
    "dark": "#1E40AF"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Seemodo App",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "seemodo/introduction",
              "seemodo/getting-started"
            ]
          },
          {
            "group": "Core Features",
            "pages": [
              "seemodo/visual-designer",
              "seemodo/ai-coder",
              "seemodo/cloud"
            ]
          },
          {
            "group": "Technical Reference",
            "pages": [
              "seemodo/architecture",
              "seemodo/sandbox",
              "seemodo/api-reference",
              "seemodo/configuration"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "seemodo/credits",
              "seemodo/troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "Docs Guide",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API Examples",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Seemodo App",
          "href": "https://seemodo.ai",
          "icon": "rocket"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/seemodo",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@seemodo.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://seemodo.ai/start"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/seemodo",
      "github": "https://github.com/seemodo"
    }
  }
