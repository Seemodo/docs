            "pages": [
              "seemodo/architecture",
              "seemodo/sandbox",
              "seemodo/api-reference",
              "seemodo/configuration"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "seemodo/credits",
              "seemodo/troubleshooting"
            ]
          }
        ]
      },
      {
