    "global": {
      "anchors": [
        {
          "anchor": "Seemodo App",
          "href": "https://seemodo.ai",
          "icon": "rocket"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/seemodo",
          "icon": "github"
        }
      ]
